POST
	localhost:8083/employee/
	Request: 	
	{
	    "firstName" : "FirstName2",
	    "lastName"  : "LastName2",
	    "email" : "email2@gmail.com",
	    "department" : {
	        "departmentName" : "IT2",
	        "departmentAddress" : "address2"
	    }
	}
	Response:
	{
	    "id": "4377a34b-d5b1-4249-92ae-2ab1a9ef5015",
	    "firstName": "FirstName2",
	    "lastName": "LastName2",
	    "email": "email2@gmail.com",
	    "department": {
	        "departmentName": "IT2",
	        "departmentAddress": "address2"
	    }
	}
	
